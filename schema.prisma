// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Admin {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  isAdmin   Boolean   @default(true)
  createdAt DateTime? @default(now())
}

model User {
  id                     String         @id @default(cuid())
  tid                    Int            @unique
  firstName              String         @default("")
  lastName               String         @default("")
  language               String         @default("")
  createdAt              DateTime?      @default(now())
  from_Subscription_user Subscription[] @relation("Subscription_user")
}

model Channel {
  id                        String         @id @default(cuid())
  tid                       Int            @unique
  username                  String         @unique @default("")
  title                     String         @default("")
  createdAt                 DateTime?      @default(now())
  from_Subscription_channel Subscription[] @relation("Subscription_channel")
}

model Subscription {
  id        String    @id @default(cuid())
  user      User?     @relation("Subscription_user", fields: [userId], references: [id])
  userId    String?   @map("user")
  channel   Channel?  @relation("Subscription_channel", fields: [channelId], references: [id])
  channelId String?   @map("channel")
  createdAt DateTime? @default(now())

  @@index([userId])
  @@index([channelId])
}
